header.page-header
	// RD Navbar
	.rd-navbar-wrap
		nav.rd-navbar.rd-navbar-default.rd-navbar-fluid(data-layout='rd-navbar-fixed', data-sm-layout='rd-navbar-fixed', data-md-layout='rd-navbar-static', data-md-device-layout='rd-navbar-fixed', data-lg-layout='rd-navbar-static', data-lg-device-layout="rd-navbar-static",  data-md-stick-up-offset="80px", data-lg-stick-up-offset="46px" data-stick-up='true' data-sm-stick-up='true' data-md-stick-up='true' data-lg-stick-up='true')


			.rd-navbar-collapse-toggle(data-rd-navbar-toggle='.rd-navbar-collapse')
				span

			.rd-navbar-inner
				// RD Navbar Panel
				.rd-navbar-panel
					// RD Navbar Toggle
					button.rd-navbar-toggle(data-rd-navbar-toggle='.rd-navbar-nav-wrap')
						span
					// RD Navbar Brand
					.rd-navbar-brand
						a.brand-name(href='index.html')
							img(src="images/brand.png", width="222", height="41", alt="")

				.rd-navbar-aside-right
					.rd-navbar-nav-wrap
						include ../_navigation

				.rd-navbar-soc.rd-navbar-collapse
					ul.list-inline
						li
							a(href='#').icon.fa-facebook
						li
							a(href='#').icon.fa-google-plus
						li
							a(href='#').icon.fa-linkedin
						li
							a(href='#').icon.fa-twitter